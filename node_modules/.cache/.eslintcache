ses\r\n      if(likedCourses.length === 0){\r\n        setlikedCourses([course.id]);\r\n      }\r\n\r\n      else{\r\n        //non empty\r\n        setlikedCourses((prev) => [...prev , course.id]);\r\n      }\r\n      toast.success(\"Liked Success\");\r\n    }\r\n  }\r\n  return (\r\n    <div className=\"w-[300px] bg-bgDark rounded-md overflow-hidden bg-opacity-80\">\r\n\r\n      <div className=\"relative\">\r\n        <img src={course.image.url}></img>\r\n\r\n      <div  className=\"rounded-full absolute w-[40px] h-[40px] \r\n      bg-white right-2 bottom-[-12px] grid place-items-center\">\r\n\r\n        <button onClick={clickHandler}>\r\n         {\r\n          !likedCourses.includes(course.id) ? (<FcLikePlaceholder fontSize=\"1.75rem\" />) : (<FcLike fontSize=\"1.75rem\" />)\r\n        }\r\n        </button>\r\n      \r\n      </div>\r\n\r\n      </div>\r\n\r\n      <div className='p-4'>\r\n        <p className=\"text-white font-semibold text-lg leading-6\">{course.title}</p>\r\n        <p className=\"text-white mt-2 \">\r\n        {\r\n          course.description.length > 100 ? (course.description.substring(0,100)+\"...\") : (course.description)\r\n        }\r\n        </p>\r\n      </div>\r\n    \r\n    </div>\r\n  )\r\n}\r\n\r\nexport default Card;","C:\\Users\\ACER\\Desktop\\DOT_React_practice_files\\top-course-starter\\src\\Components\\Spinner.js",[],[],{"ruleId":"45","severity":1,"message":"46","line":35,"column":9,"nodeType":"47","endLine":35,"endColumn":37},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement"]